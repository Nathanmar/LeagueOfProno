generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model matches {
  id            String    @id @db.VarChar(255)
  team_a        String?   @db.VarChar(255)
  team_b        String?   @db.VarChar(255)
  team_a_logo   String?
  team_b_logo   String?
  match_date    DateTime? @db.Timestamptz(6)
  tournament    String?   @db.VarChar(255)
  status        String?   @db.VarChar(100)
  winner        String?   @db.VarChar(255)
  score_a       Int?
  score_b       Int?
  created_date  DateTime? @db.Timestamptz(6)
  updated_date  DateTime? @db.Timestamptz(6)
  created_by_id String?   @db.VarChar(255)
  created_by    String?   @db.VarChar(255)
  is_sample     Boolean?
}
